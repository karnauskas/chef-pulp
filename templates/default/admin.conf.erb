[server]
host: <%= @conf['server']['host'] %>
port: <%= @conf['server']['port'] %>
api_prefix: <%= @conf['server']['api_prefix'] %>
verify_ssl: <%= @conf['server']['verify_ssl'].to_s.capitalize %>
ca_path: <%= @conf['server']['ca_path'] %>
upload_chunk_size: <%= @conf['server']['upload_chunk_size'] %>

[client]
role: <%= @conf['client']['role'] %>

[filesystem]
extensions_dir: <%= @conf['filesystem']['extensions_dir'] %>
id_cert_dir: <%= @conf['filesystem']['id_cert_dir'] %>
id_cert_filename: <%= @conf['filesystem']['id_cert_filename'] %>
upload_working_dir: <%= @conf['filesystem']['upload_working_dir'] %>

[logging]
filename: <%= @conf['logging']['filename'] %>
call_log_filename: <%= @conf['logging']['call_log_filename'] %>

[output]
poll_frequency_in_seconds: <%= @conf['output']['poll_frequency_in_seconds'] %>
enable_color: <%= @conf['output']['enable_color'].to_s %>
wrap_to_terminal: <%= @conf['output']['wrap_to_terminal'].to_s %>
wrap_width: <%= @conf['output']['wrap_width'] %>
